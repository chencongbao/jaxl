{
    "name":        "chencongbao/jaxl",
    "type":        "library",
    "description": "基于网上的jaxl开源类库，修改了几个bug",
    "keywords":    ["xmpp", "jabber", "http", "asynchronous", "non blocking", "event loop", "php", "jaxl", "abhinavsingh"],
    "homepage":    "http://jaxl.readthedocs.org/en/latest/index.html",
    "license":     "BSD-3-Clause",
    "version":      "v1.0",
    "support": {
        "forum":   "https://groups.google.com/forum/#!forum/jaxl",
        "issues":  "https://github.com/jaxl/JAXL/issues",
        "source":  "https://github.com/jaxl/JAXL"
    },
    "authors": [
        {
            "name": "bob",
            "homepage": "https://www.cfdlm.com/"
        }
    ],
    "require": {
        "php": ">=5.2.4",
        "ext-curl": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-sockets": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^3.7.0",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "ext-pcntl": "Interrupt JAXL with signals"
    },
    "autoload": {
        "classmap": [
            "src/jaxl"
        ],
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "bin": ["jaxlctl"],
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
